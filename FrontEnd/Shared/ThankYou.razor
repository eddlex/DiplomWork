@page "/ThankYou"

<style>
    body {
        background-color: lightgrey;
        opacity: 0.8;
    }
</style>

<MudContainer MaxWidth="MaxWidth.Medium" Class="my-6" style="padding-top: 20px;">
    <MudPaper Elevation="13" Class="p-6 mx-auto" Style="border-radius: 20px;">
        <MudText Typo="Typo.h4" GutterBottom="true" Align="Align.Center" Color="Color.Primary" style="padding-top: 10px;">
            Thank You!
        </MudText>
        <MudText Typo="Typo.body1" Align="Align.Center" style="padding-bottom: 10px;">
            Your feedback has been submitted successfully. <br>Thank you for participating.
        </MudText>
    </MudPaper>
</MudContainer>


